<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>

    <clientDataStore type="Sitecore.Data.$(database).$(database)ClientDataStore, Sitecore.Kernel"
                 set:type="Sitecore.Support.Data.$(database).$(database)ClientDataStore, Sitecore.Support.176278">
    </clientDataStore>

    <settings>
      <!--
          CLIENT DATA - MAXIMUM BATCH SIZE
          This setting specifies the maximum number of records that CompactData method can process in a single transaction.
          Default: 1000      
       -->
      <setting name="ClientData.MaximumBatchSize" value="1000"/>
    </settings>

  </sitecore>
</configuration>